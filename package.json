{
	"name": "training-log",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng": "nx",
		"postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
		"start": "nx serve",
		"build": "nx build",
		"test": "nx test",
		"lint": "npx nx run-many --target=lint --all",
		"pg:start": "docker-compose up -d",
		"db:migrate": "npx prisma migrate dev --preview-feature --skip-generate --name \"init\"",
		"db:seed": "npx prisma db seed",
		"test:api": "npm run db:seed && node --expose-gc node_modules/@nrwl/cli/bin/nx test api --skip-nx-cache -- --runInBand --logHeapUsage",
		"cypress": "npx cypress open -P apps/portal-e2e",
		"cypress:run": "npx cypress run -P apps/portal-e2e --browser chrome",
		"serve:e2e": "npx nx run-many --target=serve --projects=api,portal --parallel=2",
		"e2e": "start-server-and-test serve:e2e 4200 cypress:run"
	},
	"private": true,
	"prisma": {
		"schema": "apps/api/prisma/schema.prisma",
		"seed": "ts-node apps/api/prisma/seed.ts"
	},
	"dependencies": {
		"@angular/animations": "~13.3.0",
		"@angular/cdk": "^13.3.5",
		"@angular/common": "~13.3.0",
		"@angular/compiler": "~13.3.0",
		"@angular/core": "~13.3.0",
		"@angular/forms": "~13.3.0",
		"@angular/material": "^13.3.5",
		"@angular/material-moment-adapter": "^13.3.7",
		"@angular/platform-browser": "~13.3.0",
		"@angular/platform-browser-dynamic": "~13.3.0",
		"@angular/router": "~13.3.0",
		"@nestjs/common": "^8.0.0",
		"@nestjs/core": "^8.0.0",
		"@nestjs/jwt": "^8.0.0",
		"@nestjs/passport": "^8.2.1",
		"@nestjs/platform-express": "^8.0.0",
		"@nestjs/platform-fastify": "^8.4.4",
		"@nrwl/angular": "^14.8.6",
		"@prisma/client": "^3.12.0",
		"bcrypt": "^5.0.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"luxon": "^2.3.2",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "~7.4.0",
		"tslib": "^2.0.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~13.3.0",
		"@angular-eslint/eslint-plugin": "~13.1.0",
		"@angular-eslint/eslint-plugin-template": "~13.1.0",
		"@angular-eslint/template-parser": "~13.1.0",
		"@angular/cli": "~13.3.0",
		"@angular/compiler-cli": "~13.3.0",
		"@angular/language-service": "~13.3.0",
		"@nestjs/schematics": "^8.0.0",
		"@nestjs/testing": "^8.0.0",
		"@nrwl/cli": "^14.8.6",
		"@nrwl/cypress": "^14.8.6",
		"@nrwl/devkit": "14.0.5",
		"@nrwl/eslint-plugin-nx": "^14.8.6",
		"@nrwl/jest": "14.0.5",
		"@nrwl/linter": "14.0.5",
		"@nrwl/nest": "^14.8.6",
		"@nrwl/node": "^14.8.6",
		"@nrwl/workspace": "^14.8.6",
		"@types/bcrypt": "^5.0.0",
		"@types/jest": "27.4.1",
