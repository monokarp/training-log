{
	"name": "training-log",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng": "nx",
		"postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
		"start": "nx serve",
		"build": "nx build",
		"test": "nx test",
		"lint": "npx nx run-many --target=lint --all",
		"pg:start": "docker-compose up -d",
		"db:migrate": "npx prisma migrate dev --preview-feature --skip-generate --name \"init\"",
		"db:seed": "npx prisma db seed",
		"test:api": "npm run db:seed && node --expose-gc node_modules/@nrwl/cli/bin/nx test api --skip-nx-cache -- --runInBand --logHeapUsage",
		"cypress": "npx cypress open -P apps/portal-e2e",
		"cypress:run": "npx cypress run -P apps/portal-e2e --browser chrome",
		"serve:e2e": "npx nx run-many --target=serve --projects=api,portal --parallel=2",
		"e2e": "start-server-and-test serve:e2e 4200 cypress:run"
	},
	"private": true,
	"prisma": {
		"schema": "apps/api/prisma/schema.prisma",
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} apps/api/prisma/seed.ts"
	},
	"dependencies": {
